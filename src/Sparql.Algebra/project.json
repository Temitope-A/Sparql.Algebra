{
  "name": "Sparql.Algebra",
  "title": "Sparql Algebra",
  "authors": [ "Temitope Ajileye" ],
  "language": "en-GB",
  "copyright": "MIT",
  "brief": "Sparql Query Language algebra specification",
  "description": "Sparql Query Language algebra specification",
  "version": "0.2.10",
  "versions": [
    {
      "Version": "0.2",
      "Description": "Updated version"
    }
  ],

  "dependencies": {
    "Microsoft.Extensions.Configuration": "1.0.0",
    "Microsoft.Extensions.Configuration.Abstractions": "1.0.0",
    "Microsoft.Extensions.Configuration.Binder": "1.0.0",
    "Microsoft.Extensions.Configuration.FileExtensions": "1.0.0",
    "Microsoft.Extensions.Configuration.Json": "1.0.0",
    "NETStandard.Library": "1.5.0-rc2-24027"
  },

  "frameworks": {
    "netstandard1.5": {
      "imports": "dnxcore50"
    }
  },

  "buildOptions": {
    "xmlDoc": true,
    "copyToOutput": {
      "include": [
        "project.json"
      ]
    }
  },

  "packOptions": {
    "repository": {
      "type": "git",
      "url": "https://github.com/Temitope-A/Sparql.Algebra"
    }
  },

  "scripts": {
    "postcompile": [
      "dotnet pack --output C:\\LibraryPackages --no-build --configuration %compile:Configuration%",
      "robocopy bin\\Release C:\\LibraryPackages\\%project:Name% /E /MIR"
    ]
  }
}

